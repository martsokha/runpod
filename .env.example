# RunPod SDK Configuration
# Copy this file to .env and set your actual values

# Your RunPod API key (required)
# Get from: https://www.runpod.io/console/user/settings
RUNPOD_API_KEY=your_runpod_api_key_here

# Custom API base URL (optional, defaults to https://rest.runpod.io/v1)
RUNPOD_BASE_URL=https://rest.runpod.io/v1

# Custom GraphQL API base URL (optional, defaults to https://api.runpod.io/graphql)
# Only used when graphql feature is enabled
RUNPOD_BASE_GRAPHQL_URL=https://api.runpod.io/graphql

# Request timeout in seconds (optional, defaults to 30)
RUNPOD_TIMEOUT_SECS=30
